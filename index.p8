--vfx

function explosion(x,y,r,c)
 circfill(x,y,r,c or 9)
 circfill(x,y,r-1,7)
end

function laser(x1,y1,x2,y2,w,c)
 local h=flr(w/2)
 for i=-h,h do
  local lx,ly,lx2,ly2
  local k=(y2-y1)/(x2-x1)
  lx=x1+(-1/k*(h+i))
  ly=y1+(-1/k*(h+i))
  lx2=x2-(-1/k*(h+i))
  ly2=y2+(-1/k*(h+i))
  color(7)
  if(abs(i)==h) color(c or 9)
  line(lx1,ly1,lx2,ly2)		   
 end
end
