end


function init_menu()
 _uupdate = update_menu
 _draw = draw_menu
 
	menu={}
	add(menu,"start")
	add(menu,"help")
	sel_action=1
	menu_act={}
	add(menu_act,init_game)
	add(menu_act,init_help)
end

function update_menu()
	if(btnp(â¬†ï¸)) sel_action-=1
	if(btnp(â¬‡ï¸)) sel_action+=1
	if(sel_action>#menu) then
	 sel_action=sel_action%#menu
	end
	if(sel_action<1) sel_action=#menu
 
 if(btnp(âŽ) or btnp(ðŸ…¾ï¸)) then
  menu_act[sel_action]()
 end
 if(btnp(âž¡ï¸)) cursor_speed+=1
 if(btnp(â¬…ï¸)) cursor_speed-=1
 cursor_speed=range(cursor_speed,1,5)
end

function draw_menu()
 cls()
 print("station command",32,16,8)

	for i,action in ipairs(menu) do
	 local c=6
	 if(i==sel_action) then
	  c=7
	  action=">"..action.."<"
	 end
	 print(action,32,16+i*8,c)
	end
	print("â¬†ï¸,â¬‡ï¸-up,down;âŽ,ðŸ…¾ï¸-select",
	 8,112,7
	)
	print("â¬…ï¸,âž¡ï¸-change cursor speed",
	 8,120,7
	)
	print("cursor speed: "..cursor_speed,32,56,7)
	line(32,64,128-32,64,7)
	for i=0,4 do
	 local x=32+i*16
	 local c=7
	 if(cursor_speed-1==i) c=8
	 line(x,62,x,66,c)
	end
end
